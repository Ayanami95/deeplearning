   "source": [
    "import torch\n",
    "\n",
    "dtype = torch.float\n",
    "\n",
    "x = torch.tensor([1, 1, 1], dtype=dtype)\n",
    "w = torch.tensor([1.0, 1.0, -1.5], dtype=dtype, requires_grad=True)\n",
    "\n",
    "loss = -torch.dot(x, w).sigmoid().log()\n",
    "loss.backward()\n",
    "print(loss.item())\n",
    "print(w.grad)"
   ]
  }
